{
  "name": "red_tetris_avallete_fbonhomm",
  "version": "0.0.1",
  "author": "avallete && fbonhomm",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint src",
    "client-dist": "cross-env NODE_ENV=production webpack --progress --colors",
    "client-dev": "cross-env NODE_ENV=development webpack-dev-server --colors --hot --inline",
    "srv-dev": "cross-env NODE_ENV=development DEBUG=tetris:* babel-watch -w src src/server/main.js",
    "srv-dist": "cross-env NODE_ENV=production DEBUG=tetris:* babel src --out-dir dist",
    "srv-test": "cross-env NODE_ENV=test DEBUG=tetris:* babel-watch -w src src/server/main.js",
    "test": "cross-env NODE_ENV=test mocha --require babel-core/register --reporter spec ./test/**.test.js ./src/**.test.js",
    "coverage": "cross-env NODE_ENV=test nyc --include src --all -r text -r lcov -r text --require babel-core/register mocha ",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern dist/*",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged"
  },
  "dependencies": {
    "dotenv": "6.0.0",
    "immutable": "3.8.2",
    "lodash": "4.17.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-immutable": "4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.0.1",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-watch": "^2.0.2",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chai-enzyme": "1.0.0-beta.1",
    "cheerio": "1.0.0-rc.2",
    "cross-env": "5.2.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.0.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-mocha": "5.0.0",
    "eslint-plugin-react": "^7.10.0",
    "joi": "13.4.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "webpack": "^4.14.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.10.0"
  },
  "nyc": {
    "exclude": [
      "build"
    ]
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-0"
    ]
  }
}
